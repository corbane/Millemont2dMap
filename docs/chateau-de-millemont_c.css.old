@charset "UTF-8";
.str1 {stroke:black;stroke-width:8.49;stroke-miterlimit:10}
.str0 {stroke:black;stroke-width:8.5;stroke-miterlimit:10}
.fil0 {fill:none;fill-rule:nonzero}

svg, g {
    pointer-events: all;
}


.mmmfest.map2d-background {
    transition: all 0.5s;
    opacity: 1;
}
.mmmfest.map2d-background.ghost {
    opacity: 0.5;
}

/* Map regions */

.mmmfest.map2 {
    
}

.mmmfest.map2d-image {
    transition: all 0.5s;
}
.mmmfest.map2d-image.ghost {
    opacity: 0;
}
.mmmfest.map2d-image.ghost.selected {
    opacity: 1;
}

.mmmfest.map2d-path.selected {
    stroke-width: 10 !important;
    stroke: white;
    filter: url(#blur-filter);
}
.mmmfest.map2d-path.ghost {
    stroke-width: 0;
}
.mmmfest.map2d-path.ghost.selected {
    stroke-width: 1;
}

